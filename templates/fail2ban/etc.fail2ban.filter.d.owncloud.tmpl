# ownCloud v9+
# https://github.com/AykutCevik/owncloud-fail2ban

[INCLUDES]
before = common.conf

[Definition]
_daemon = owncloud
failregex = {"reqId":".*","remoteAddr":".*","app":"core","message":"Login failed: '.*' \(Remote IP: '<HOST>', X-Forwarded-For: '.*'\)","level":2,"time":".*","method":".*}
